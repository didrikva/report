    "autoload": {
        "psr-4": {"Didrik\\": "src/"}
    },
    "scripts": {
        "clean": "rm -rf build .phpunit.result.cache .phpunit.cache",
        "clean-all": [
            "@clean",
            "rm -rf vendor composer.lock"
        ],
        "phpunit": "XDEBUG_MODE=coverage vendor/bin/phpunit"
    },
    "require-dev": {
        "php": "^8.2",
        "phpunit/phpunit": "^11"
    }
}
